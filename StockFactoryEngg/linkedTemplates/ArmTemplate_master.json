{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "StockFactoryEngg"
		},
		"data_ipl_bronze_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'data_ipl_bronze'"
		},
		"data_ipl_bronze_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ipldatastorage.dfs.core.windows.net/"
		},
		"file_watcher_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/b36cb301-935a-4584-b953-bb276098e8d2/resourceGroups/StockDataEngg/providers/Microsoft.Storage/storageAccounts/ipldatastorage"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "StockFactoryEngg_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"data_ipl_bronze_accountKey": {
						"value": "[parameters('data_ipl_bronze_accountKey')]"
					},
					"data_ipl_bronze_properties_typeProperties_url": {
						"value": "[parameters('data_ipl_bronze_properties_typeProperties_url')]"
					},
					"file_watcher_properties_typeProperties_scope": {
						"value": "[parameters('file_watcher_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}